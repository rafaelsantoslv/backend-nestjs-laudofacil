generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Usuarios {
  id              Int               @id @default(autoincrement())
  email           String            @unique
  password        String
  username        String

  createdAt       DateTime          @default(now())
  updatedAt       DateTime          @default(now()) @updatedAt
}